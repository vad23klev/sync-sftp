{
  "name": "sync-sftp",
  "publisher": "vad23klev",
  "displayName": "Sync SFTP",
  "description": "Sync your project with remote using SFTP",
  "icon": "./icons/icon.png",
  "version": "0.4.33",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "url": "https://github.com/vad23klev/sync-sftp"
  },
  "activationEvents": [
    "workspaceContains:**/.sync-sftp.json"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sync-sftp.detectDifferences",
        "category": "Sync SFTP",
        "title": "Detect differences",
        "icon": "$(info)"
      },
      {
        "command": "sync-sftp.makeEqual",
        "category": "Sync SFTP",
        "title": "Make equal",
        "icon": "$(remote)"
      },
      {
        "command": "sync-sftp.reconnect",
        "category": "Sync SFTP",
        "title": "Reconnect",
        "icon": "$(debug-disconnect)"
      },
      {
        "command": "sync-sftp.reupload",
        "category": "Sync SFTP",
        "title": "Reupload",
        "icon": "$(cloud-upload)"
      },
      {
        "command": "sync-sftp.clearLog",
        "category": "Sync SFTP",
        "title": "Clear",
        "icon": "$(clear-all)"
      },
      {
        "command": "sync-sftp.upload",
        "title": "Sync SFTP: Upload file"
      }
    ],
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "sync-sftp.logView",
          "name": "Sync SFTP"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "sync-sftp.detectDifferences",
          "group": "navigation@0",
          "when": "view == sync-sftp.logView"
        },
        {
          "command": "sync-sftp.makeEqual",
          "group": "navigation@1",
          "when": "view == sync-sftp.logView"
        },
        {
          "command": "sync-sftp.reupload",
          "group": "navigation@2",
          "when": "view == sync-sftp.logView"
        },
        {
          "command": "sync-sftp.clearLog",
          "group": "navigation@3",
          "when": "view == sync-sftp.logView"
        }
      ],
      "explorer/context": [
        {
          "command": "sync-sftp.upload",
          "group": "sync-sftp"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint"
  },
  "devDependencies": {
    "eslint": "^8.57.0"
  },
  "dependencies": {
    "node-ssh": "^13.2.0",
    "node-watch": "^0.7",
    "ping": "^0.4.4",
    "relaxed-json": "^1.0.0",
    "rsync2": "^2.0.2"
  }
}
