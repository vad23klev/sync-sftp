{
  "name": "sync-sftp",
  "publisher": "vad23klev",
  "displayName": "Sync SFTP",
  "description": "Sync your project with remote using SFTP",
  "icon": "./icons/icon.png",
  "version": "0.3.30",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "url": "https://github.com/vad23klev/sync-sftp"
  },
  "activationEvents": [
    "workspaceContains:**/.sync-sftp.json"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sync-sftp.reloadConfig",
        "category": "Sync SFTP",
        "title": "Sync SFTP: Reload Configuration",
        "icon": "$(debug-restart)"
      },
      {
        "command": "sync-sftp.clearLog",
        "category": "Sync SFTP",
        "title": "Sync SFTP: Clear",
        "icon": "$(clear-all)"
      },
      {
        "command": "sync-sftp.upload",
        "title": "Sync SFTP: Upload file"
      }
    ],
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "sync-sftp.logView",
          "name": "Sync SFTP"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "sync-sftp.reloadConfig",
          "group": "navigation",
          "when": "view == sync-sftp.logView"
        },
        {
          "command": "sync-sftp.clearLog",
          "group": "navigation",
          "when": "view == sync-sftp.logView"
        }
      ],
      "explorer/context": [
        {
          "command": "sync-sftp.upload",
          "group": "sync-sftp"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint"
  },
  "devDependencies": {
    "eslint": "^8.57.0"
  },
  "dependencies": {
    "node-ssh": "^13.2.0",
    "node-watch": "^0.7",
    "relaxed-json": "^1.0.0"
  }
}
